{
    "name": "yansongda/laravel-parsedown",
    "description": "Convert Markdown To Html With Laravel",
    "keywords": ["markdown", "html", "laravel", "package"],
    "support": {
        "issues": "https://github.com/yansongda/laravel-parsedown/issues",
        "source": "https://github.com/yansongda/laravel-parsedown"
    },
    "authors": [
        {
            "name": "yansongda",
            "email": "me@yansongda.cn"
        }
    ],
    "require": {
        "illuminate/support": "^5.0|^6.0",
        "erusev/parsedown": "~1.7"
    },
    "require-dev": {
        "orchestra/testbench": "3.9.x-dev@dev"
    },
    "autoload": {
        "psr-4": {
            "Yansongda\\LaravelParsedown\\": "src"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Yansongda\\LaravelParsedown\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Yansongda\\LaravelParsedown\\ParsedownServiceProvider"
            ],
            "aliases": {
                "LaravelParsedown": "Yansongda\\LaravelParsedown\\Facades\\Parsedown"
            }
        }
    },
    "license": "MIT",
    "minimum-stability": "dev",
    "prefer-stable": true
}
